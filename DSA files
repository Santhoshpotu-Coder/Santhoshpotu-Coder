print('pattern')

for i in range(1,6):
    print('#'*i)

print('------------------')

for i in range(5,0,-1):
    print('#'*i)

print('------------------')

for i in range(1,6):
    print(' '*(6-i) +'# '*i)
    
print('------------------')
    
for i in range(5,0,-1):
    print(' '*(5-i) +'# '*i +''*(5-i))
